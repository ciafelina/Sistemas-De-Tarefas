  object ConsTaksC: TQuery
    DatabaseName = 'Base'
    SQL.Strings = (
      
        'select Nome as Name,Tipo as Type,Descricao as Description,Obs as' +
        ' Observation,Horas as Hour,DatTarefas as Date from Tarefas where' +
        ' Statu = '#39'V'#39)
    Left = 544
    object ConsTaksCName: TStringField
      FieldName = 'Name'
      FixedChar = True
      Size = 50
    end
    object ConsTaksCType: TStringField
      FieldName = 'Type'
      FixedChar = True
    end
    object ConsTaksCDescription: TStringField
      FieldName = 'Description'
      FixedChar = True
      Size = 30
    end
    object ConsTaksCObservation: TStringField
      FieldName = 'Observation'
      FixedChar = True
      Size = 50
    end
    object ConsTaksCHour: TStringField
      FieldName = 'Hour'
      FixedChar = True
      Size = 10
    end
    object ConsTaksCDate: TDateTimeField
      FieldName = 'Date'
    end
  end
